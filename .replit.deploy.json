{
  "run": "NODE_ENV=production PORT=8080 node server/deployment-server.js",
  "entrypoint": "server/deployment-server.js",
  "build": "npm run build",
  "exclude": [
    "attached_assets",
    "backup_assets",
    "backup_text",
    "uploads",
    "node_modules/.cache",
    "node_modules/typescript",
    "node_modules/@types",
    "node_modules/esbuild",
    ".git",
    "client/src",
    "server/src",
    "*.md",
    "*.sh",
    "*.cjs",
    "*.ts",
    "*.tsx",
    "!server/deployment-server.js",
    "!dist/**/*.js"
  ],
  "port": 8080
}